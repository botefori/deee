{% extends '::front/front.html.twig' %}

{% block title %}{{parent()}} Demande Envoyée {% endblock %}
{%block carsl %}
{%endblock %}

{% block body -%}
    <div id="wrapper">
    <div class="container-fluid">
        <div class="row">
            <div class="col-md-2"></div>
            <div class="col-md-8">
                <br>
                <h2>Nous vous recontacterons très bientôt </h2>
                <br>
                <table id="recap-table" class="table">
                    <tbody>
                        <tr>
                            <th>Company</th>
                            <td>{{ entity.company }}</td>
                        </tr>
                        <tr>
                            <th>Contact</th>
                            <td>{{ entity.contact }}</td>
                        </tr>
                        <tr>
                            <th>Address</th>
                            <td>{{ entity.address }}</td>
                        </tr>
                        <tr>
                            <th>City</th>
                            <td>{{ entity.city }}</td>
                        </tr>
                        <tr>
                            <th>Postalcode</th>
                            <td>{{ entity.postalCode }}</td>
                        </tr>
                        <tr>
                            <th>Phone</th>
                            <td>{{ entity.phone }}</td>
                        </tr>
                        <tr>
                            <th>Email</th>
                            <td>{{ entity.email }}</td>
                        </tr>
                        <tr>
                            <th>Message</th>
                            <td>{{ entity.message }}</td>
                        </tr>
                    </tbody>
                </table>
            </div>
            <div class="col-md-2"></div>
        </div>
    </div>
    </div>
{% endblock %}
