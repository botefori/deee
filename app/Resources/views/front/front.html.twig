<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
        <title>{% block title %} {% endblock %}</title>
        {% block stylesheets %}
            {%stylesheets filter='cssrewrite'
               '%Kernel.root_dir%/../vendor/twbs/bootstrap/dist/css/bootstrap.css'
               '%Kernel.root_dir%/../vendor/twbs/bootstrap/dist/css/bootstrap-theme.css'
               '@HaccoDeeeBundle/Resources/public/css/front/custom-front.css'
             %}
             <link rel="stylesheet" href="{{asset_url}}" type="text/css"/>
             {%endstylesheets%}
        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
    </head>
    <body>
        <header>
            <nav class="navbar navbar-default navbar-fixed-top" role="navigation"> <!-- Navbar start -->
                <div class="container" id="menu-container">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#collapse" aria-expanded="false">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="{{ path('homepage') }}"><h1>  <span class="subhead"></span></h1></a>
                    </div><!-- navbarheader -->
                    <div class="collapse navbar-collapse" id="collapse">
                        <ul class="nav navbar-nav navbar-right">
                            <li><a  href="{{ path('homepage') }}">Accueil</a></li>
                            <li><a href="{{ path('Our_services') }}">Nos Prestations</a></li>
                            <li><a href="{{ path('devissetup_new') }}">Devis</a></li>
                            <li><a href="{{ path('customers_new') }}">Contact & Renseignement</a></li>
                        </ul>
                    </div><!-- Collapse -->
                </div> <!-- Container end -->
            </nav> <!-- Navbar End -->
            {%block carsl %}
                
            {%endblock %}
        </header>
        {% block body %}
        {% endblock %}
        {% block javascripts %}
            {% javascripts
               '%Kernel.root_dir%/../vendor/components/jquery/jquery.js'
               '%Kernel.root_dir%/../vendor/twbs/bootstrap/dist/js/bootstrap.js'
               '@HaccoDeeeBundle/Resources/public/js/front/custom.js'
            %}
            <script src="{{asset_url}}" type="text/javascript"></script>
            {% endjavascripts %}
        {% endblock %}
    </body>
</html>
